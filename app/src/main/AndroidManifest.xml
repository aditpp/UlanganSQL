    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.03_AlvanulanganSQL">
        <activity android:name="com.ulangan.adityosql.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ulangan.adityosql.MainActivity"/>
        <activity android:name="com.ulangan.adityosql.ListActivity"/>
    </application>

</manifest>